{
    "name": "fuelviews/laravel-sabhero-estimator",
    "description": "A comprehensive painting project estimator package for Laravel applications with Livewire and FilamentPHP",
    "keywords": [
        "fuelviews",
        "laravel",
        "laravel-sabhero-estimator",
        "painting",
        "estimator",
        "livewire",
        "filament",
        "calculator",
        "project-estimation"
    ],
    "homepage": "https://github.com/fuelviews/laravel-sabhero-estimator",
    "license": "MIT",
    "authors": [
        {
            "name": "thejmitchener",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "filament/filament": "^4.1",
        "illuminate/contracts": "^10.0||^11.0||^12.0",
        "illuminate/database": "^10.0||^11.0||^12.0",
        "illuminate/http": "^10.0||^11.0||^12.0",
        "illuminate/support": "^10.0||^11.0||^12.0",
        "illuminate/validation": "^10.0||^11.0||^12.0",
        "livewire/livewire": "^3.0",
        "ralphjsmit/laravel-glide": "*",
        "spatie/laravel-package-tools": "^1.92"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.1.1||^7.10.0",
        "orchestra/testbench": "^10.0.0||^9.0.0||^8.22.0",
        "pestphp/pest": "^3.0||^2.34",
        "pestphp/pest-plugin-arch": "^3.0||^2.7",
        "pestphp/pest-plugin-laravel": "^3.2||^2.3",
        "rector/rector": "^2.0",
        "driftingly/rector-laravel": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Fuelviews\\SabHeroEstimator\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Fuelviews\\SabHeroEstimator\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Fuelviews\\SabHeroEstimator\\SabHeroEstimatorServiceProvider"
            ],
            "aliases": {
                "SabHeroEstimator": "Fuelviews\\SabHeroEstimator\\Facades\\SabHeroEstimator"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
